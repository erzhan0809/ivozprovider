<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Ast\Domain\Model\VoicemailMessage\VoicemailMessageAbstract" table="voicemailmessages_abstract">
    <field name="dir" type="string" column="dir" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="msgnum" type="integer" column="msgnum" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="context" type="string" column="context" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="macrocontext" type="string" column="macrocontext" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="callerid" type="string" column="callerid" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="origtime" type="integer" column="origtime" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="duration" type="integer" column="duration" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="recording" type="string" column="recording" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <embedded name="recordingFile" class="Ivoz\Ast\Domain\Model\VoicemailMessage\RecordingFile" use-column-prefix="false" />
    <field name="flag" type="string" column="flag" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="category" type="string" column="category" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailboxuser" type="string" column="mailboxuser" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailboxcontext" type="string" column="mailboxcontext" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="msgId" type="string" column="msg_id" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="user" target-entity="Ivoz\Provider\Domain\Model\User\UserInterface" fetch="LAZY">
      <join-columns>
        <join-column name="userId" referenced-column-name="id" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="residentialDevice" target-entity="Ivoz\Provider\Domain\Model\ResidentialDevice\ResidentialDeviceInterface" fetch="LAZY">
      <join-columns>
        <join-column name="residentialDeviceId" referenced-column-name="id" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
